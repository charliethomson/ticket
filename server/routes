
getters

WorkorderFilter {
    id: Option<i64>,
    active: Option<bool>,
    origin: Option<i64>,
    created: Option<i32>,
    quoted: Option<i32>,
    workorder_status: Option<Vec<i32>>,
    travel_status: Option<i32>,
    location: Option<String>,
    customer: Option<i64>,
    device: Option<i64>,
    brief: Option<String>,
}

DeviceFilter {
    id: Option<i64>,
    serial_no: Option<String>,
    device_name: Option<String>,
    customer: Option<i64>, // Customer ID
    password: Option<String>,
}

StoreFilter {
    id: Option<i64>,
    contact_name: Option<String>,
    phone_number: Option<String>,
    email_address: Option<String>,
    address: Option<String>,
    city: Option<String>,
    state: Option<String>,
    zip: Option<i32>,
}

CustomerFilter {
    id: Option<i64>,
    first_name: Option<String>,
    last_name: Option<String>,
    phone_number: Option<String>,
    email_address: Option<String>,
}

UserFilter {
    id: Option<i64>,
    first_name: Option<String>,
    last_name: Option<String>,
    email_address: Option<String>,
}

NoteFilter {
    id: Option<i64>,
    workorder_id: Option<i64>,
}